					"listen": "test",
					"script": {
						"id": "d08441bf-d03f-4f35-b7bf-5855094fc34d",
						"exec": [
							"pm.test(\"CP18 - Validar que se permita consultar por un State valido - Status code is 200\", ()=> {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"CP18 - Validar sucursales retornadas y la cantidad de registros correspondan a las sucursales asociadas State ingresado.\", ()=> {\r",
							"    const expect_stateEnvironment = pm.variables.get(\"state\").toUpperCase();\r",
							"    const expect_cantsuc = pm.variables.get(\"cantsuc\");\r",
							"    const body = pm.response.json();\r",
							"\r",
							"    for (let i=0; i<body.length; i++){\r",
							"        \r",
							"        pm.expect(body[i].State.toUpperCase()).to.include(expect_stateEnvironment);        \r",
							"    }\r",
							"    pm.expect(body.length).to.equal(expect_cantsuc);       \r",
							"});"
						],
						"type": "text/javascript"
					}
				},
